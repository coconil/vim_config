 {   "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [".ccls"],
      "initializationOptions": {
         "cache": {
           "directory": "c:/work/tmp/ccls"
         }
       }
    }
  },
  "list.insertMappings": {
  "<C-n>": "normal:j",
  "<C-p>": "normal:k",
},
  "list.normalMappings": {
  "q": "do:exit",
},
  }

